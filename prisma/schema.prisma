model booking_schedules {
  id                     BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  app_type               String    @db.VarChar(255)
  card_id                String?   @db.VarChar(255)
  grade_id               String?   @db.VarChar(255)
  declaration_date       String?   @db.VarChar(255)
  trans_date             String?   @db.VarChar(255)
  expired_date           String?   @db.VarChar(255)
  appointment_date       String?   @db.VarChar(255)
  time_start_appointment String?   @db.VarChar(255)
  time_end_appointment   String?   @db.VarChar(255)
  gst_id                 String?   @db.VarChar(255)
  transaction_amount_id  String?   @db.VarChar(255)
  grand_total            String?   @db.VarChar(255)
  Status_app             String?   @db.VarChar(255)
  Status_draft           String?   @db.VarChar(255)
  paymentby              String?   @db.VarChar(255)
  status_payment         String?   @db.VarChar(255)
  receiptNo              String?   @unique(map: "booking_schedules_receiptno_unique") @db.VarChar(255)
  nric                   String    @db.VarChar(255)
  passid                 String    @unique(map: "passid_UNIQUE") @db.VarChar(255)
  resubmission_date      String?   @db.VarChar(255)
  created_at             DateTime? @db.Timestamp(0)
  updated_at             DateTime? @db.Timestamp(0)
  array_grade            String?   @db.VarChar(255)
  bsoc                   String?   @db.VarChar(255)
  ssoc                   String?   @db.VarChar(255)
  sssc                   String?   @db.VarChar(255)
  netstxnref             String?   @db.VarChar(255)
  txnrand                String?   @db.VarChar(255)
  stagerespcode          String?   @db.VarChar(255)
  netsTxnStatus          String?   @db.VarChar(255)
  licence_status         String?   @db.VarChar(255)
  card_issue             String?   @default("N") @db.VarChar(255)
  data_barcode_paynow    String?   @db.LongText
  Import_Status          String?   @db.VarChar(1)
  TaxReceipt             String?   @db.VarChar(20)
  TR_RTT                 String?   @db.VarChar(3)
  TR_CSSPB               String?   @db.VarChar(3)
  TR_CCTC                String?   @db.VarChar(3)
  TR_HCTA                String?   @db.VarChar(3)
  TR_X_RAY               String?   @db.VarChar(3)
  TR_AVSO                String?   @db.VarChar(3)
  QRstring               String?   @db.VarChar(255)
  union_member           Int?
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model dateholidays {
  id           BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  date         String?   @db.VarChar(255)
  name_holiday String?   @db.VarChar(255)
  time_work    String?   @db.VarChar(255)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
}

model failed_jobs {
  id         BigInt   @id @default(autoincrement()) @db.UnsignedBigInt
  connection String   @db.Text
  queue      String   @db.Text
  payload    String   @db.LongText
  exception  String   @db.LongText
  failed_at  DateTime @default(now()) @db.Timestamp(0)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model grades {
  id          BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  card_id     String?   @db.VarChar(255)
  name        String    @db.VarChar(255)
  type        String?   @db.VarChar(255)
  bsoc        String    @db.VarChar(255)
  ssoc        String    @db.VarChar(255)
  sssc        String    @db.VarChar(255)
  created_by  String?   @db.VarChar(255)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
